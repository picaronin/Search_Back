{
    "name": "Picaron/SearchBack",
    "type": "phpbb-extension",
    "description": "Search topics which were created or answered in the last 15, 30 or 45 minutes; 1, 2, 6 or 12 hours; 1, 3, 7, 10, 15 or 20 days; 1 month (easily expandable to other values) from the event page, language page and configurable from the ACP.",
    "homepage": "",
    "version": "1.0.1",
    "time": "2016-07-20",
    "keywords": ["phpbb", "extension", "search"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Picaron",
            "homepage": "https://github.com/picaronin",
            "role": "Desarrollador Principal"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": "3.1.*"
    },
    "extra": {
        "display-name": "Search Back",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0-RC2,<3.2.*@dev"
        },
        "version-check": {
			"host": "www.siteproall.com",
			"directory": "/SearchBack",
			"filename": "searchback_version"
        }
    }
}